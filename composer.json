{
    "name": "yab/laravel-scout-mysql-driver",
    "description": "MySQL driver for Laravel Scout.",
    "license": "MIT",
    "authors": [
        {
            "name": "Damian Crisafulli",
            "email": "damian.crisafulli@troyweb.com"
        },
        {
