            "email": "matt@yabhq.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*|5.4.*|5.5.*|5.6.*",
        "laravel/scout": "^5.0"
