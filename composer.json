{
    "name": "yab/laravel-scout-mysql-driver",
    "description": "MySQL driver for Laravel Scout.",
    "license": "MIT",
    "authors": [
        {
            "name": "Damian Crisafulli",
            "email": "damian.crisafulli@troyweb.com"
        },
        {
            "name": "Matt Lantz",
            "email": "matt@yabhq.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "laravel/scout": "~6.0|~7.0|~8.0|~9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.x-dev",
        "laravel/framework": "5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0 || ^8.0",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Yab\\MySQLScout\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yab\\MySQLScout\\Providers\\MySQLScoutServiceProvider"
            ]
        }
    }
}
